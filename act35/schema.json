{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "Nombre": {
                "type": "string"
            },
            "dulzura": {
                "type": "integer",
                "minimum": 0,
                "maximum": 10
            },
            "Disponible": {
                "type": "boolean"
            },
            "Imagen": {
                "type": "string",
                "format": "uri"
            }
        },
        "required": ["Nombre", "dulzura", "Disponible", "Imagen"]
    }
}